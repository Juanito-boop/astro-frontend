---
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Sistema de inventario para la empresa Camara de Comercio de Tunja" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/astro-frontend/favicon.svg" />
		<title set:html={title} />
	</head>
	<body>
		<slot />
	</body>
</html>